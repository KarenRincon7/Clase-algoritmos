Algoritmo codigo_9 //calculadora//
	definir contador Como Entero 
	contador<-1
	suma<-0
	definir num Como Entero
	mientras contador <= 5
		escribir "Digite un numero: "
		leer num
		suma <- suma + num 
		contador <- contador + 1
	FinMientras
	escribir "la suma total es: ", suma
FinAlgoritmo
